@article{GuidaultAndBelytschko2007,
author = {Guidault, P.-A. and Belytschko, T.},
title = {On the L2 and the H1 couplings for an overlapping domain decomposition method using Lagrange multipliers},
journal = {International Journal for Numerical Methods in Engineering},
volume = {70},
number = {3},
pages = {322-350},
keywords = {bridging-domain method, domain decomposition, multiscale, Lagrange multipliers, localized phenomena},
doi = {https://doi.org/10.1002/nme.1882},
url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/nme.1882},
eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/nme.1882},
abstract = {Abstract In this paper, a comparison of the L2 and the H1 couplings is made for an overlapping domain decomposition method using Lagrange multipliers. The analysis of the local equations arising from the formulation of the coupling of two mechanical models shows that continuous weight functions are required for the L2 coupling term whereas both discontinuous and continuous weight functions can be used for the H1 coupling. The choice of the Lagrange multiplier space is discussed and numerically studied. The paper ends with some numerical examples of an end-loaded cantilever beam and a cracked plate under tension and shear. It is shown that the continuity enforced with the H1 coupling leads to a link with a flexibility that can be beneficial for coupling a very coarse mesh with a very fine one. To limit the effect of the volume coupling on the global response, a narrow coupling zone is recommended. In this case, volume coupling tends to a surface coupling, especially with a L2 coupling. Copyright © 2006 John Wiley \& Sons, Ltd.},
year = {2007}
}


